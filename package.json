{
  "name": "strider",
  "version": "1.8.0",
  "bin": {
    "strider": "bin/strider"
  },
  "main": "main.js",
  "description": "Brilliant continuous deployment platform",
  "license": "BSD-3-Clause",
  "keywords": [
    "strider",
    "continuous integration",
    "continuous deployment",
    "testing",
    "deployment"
  ],
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Strider-CD/strider.git"
  },
  "browser": {
    "bootstrap": "./vendor/bootstrap/js/bootstrap.js",
    "sortable": "./vendor/Sortable/js/Sortable.js",
    "bootbox": "./vendor/bootbox/js/bootbox.js",
    "codemirror": "./vendor/CodeMirror/js/codemirror.js",
    "codemirror-shell": "./vendor/CodeMirror/js/shell.js",
    "timeago": "./vendor/jquery-timeago/js/jquery.timeago.js",
    "ui-codemirror": "./public/libs/ui-codemirror.js",
    "ui-bootstrap": "./public/libs/ui-bootstrap.js"
  },
  "browserify-shim": "./shim.js",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "scripts": {
    "test": "npm run build && npm run lint && npm run test-unit",
    "start": "bin/strider",
    "dev": "npm run build-debug && bin/strider",
    "pre": "mkdirp dist/scripts dist/styles",
    "vendor": "bower-installer",
    "styles": "lessc client/styles/strider.less > dist/styles/styles.css",
    "styles-debug": "lessc --source-map client/styles/strider.less dist/styles/styles.css",
    "watch": "npm run pre && watchify -e client/app.js -o dist/scripts/app.js",
    "build": "npm run pre && npm run styles && browserify -d -t [babelify --only client] -p [minifyify --output dist/scripts/bundle.map] -e client/app.js -o dist/scripts/app.js",
    "build-debug": "npm run pre && npm run styles-debug && browserify -d -t [babelify --only client] -e client/app.js -o dist/scripts/app.js",
    "test-unit": "mocha -w --recursive test/unit -R min",
    "lint": "eslint *.js lib client",
    "unitcov": "istanbul cover ./node_modules/mocha/bin/_mocha --recursive test/unit",
    "gendocs": "apidoc -i lib/ -o apidocs/",
    "debug": "node debug bin/strider --no-cluster"
  },
  "dependencies": {
    "ansi_up": "^1.3.0",
    "async": "^1.2.1",
    "bcryptjs": "^2.1.0",
    "bluebird": "^2.3.2",
    "body-parser": "^1.10.0",
    "compression": "^1.2.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^0.8.1",
    "connect-slashes": "^1.3.1",
    "cookie-parser": "^1.3.3",
    "cors": "^2.5.2",
    "debug": "^2.2.0",
    "errorhandler": "^1.3.0",
    "everypaas": "0.0.8",
    "express": "^4.10.6",
    "express-session": "^1.9.3",
    "extend": "^2.0.0",
    "global-tunnel": "^1.2.0",
    "gravatar": "^1.1.0",
    "httpcheck": "^0.2.0",
    "jade": "^0.35.0",
    "lodash": "^3.9.3",
    "md5": "^2.0.0",
    "method-override": "^2.3.0",
    "mongoose": "3.8.*",
    "morgan-debug": "^1.0.0",
    "node-rsa": "^0.4.0",
    "passport": "^0.2.0",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^1.0.2",
    "rc": "^1.0.3",
    "request": "^2.49.0",
    "semver": "^4.3.3",
    "serve-favicon": "^2.2.0",
    "socket.io": "^1.2.1",
    "socket.io-client": "^1.0.6",
    "sshpk": "^1.8.3",
    "step": "0.0.5",
    "strider-bitbucket": "^1.2.0",
    "strider-build-badge": "^0",
    "strider-cli": "^1.4.1",
    "strider-custom": "^0.6.0",
    "strider-ecosystem-client": "^1.2.1",
    "strider-email-notifier": "^0.4.2",
    "strider-env": "^0.5.1",
    "strider-extension-loader": "^0.4.5",
    "strider-git": "^0.2.0",
    "strider-github": "^2.3.0",
    "strider-gitlab": "^1.2.3",
    "strider-heroku": "^0.1.1",
    "strider-mailer": "^0.2.0",
    "strider-metadata": "^0.0.3",
    "strider-node": "^0.7.1",
    "strider-python": "^0.2.1",
    "strider-ruby": "^0.0.2",
    "strider-simple-runner": "^0.15.2",
    "strider-slack": "^2.0.0",
    "strider-ssh-deploy": "^1.0.0",
    "swig": "^0.14.0",
    "thirty-two": "0.0.2",
    "validator": "^3.22.1",
    "winston": "^1.0.0"
  },
  "devDependencies": {
    "angular": "1.4.7",
    "angular-route": "1.4.7",
    "apidoc": "^0.13.1",
    "babelify": "^6.1.2",
    "bower-installer": "^1.2.0",
    "browserify": "^11.1.0",
    "browserify-shim": "^3.8.0",
    "chai": "^1.9.2",
    "chai-as-promised": "^4.1.0",
    "eslint": "^3.0.1",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.4",
    "jquery": "^2.1.4",
    "less": "^2.2.0",
    "minifyify": "^7.3.3",
    "mkdirp": "^0.5.0",
    "mocha": "^1.21.4",
    "node-mocks-http": "^1.2.4",
    "should": "^2.1.0",
    "sinon": "^1.10.3",
    "watchify": "^3.4.0",
    "wd": "^0.3.8"
  }
}
