{
  "name": "strider",
  "version": "2.0.5-alpha.4",
  "bin": {
    "strider": "bin/strider"
  },
  "main": "dist-lib/index.js",
  "description": "Brilliant continuous deployment platform",
  "license": "BSD-3-Clause",
  "keywords": [
    "strider",
    "continuous integration",
    "continuous deployment",
    "testing",
    "deployment"
  ],
  "engines": {
    "node": ">=10.13.0 || >=11.10.1",
    "npm": ">=6.13.4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Strider-CD/strider.git"
  },
  "browser": {
    "bootstrap": "./vendor/bootstrap/js/bootstrap.js",
    "sortable": "./vendor/Sortable/js/Sortable.js",
    "bootbox": "./vendor/bootbox/js/bootbox.js",
    "codemirror": "./vendor/CodeMirror/js/codemirror.js",
    "codemirror-shell": "./vendor/CodeMirror/js/shell.js",
    "timeago": "./vendor/jquery-timeago/js/jquery.timeago.js",
    "ui-codemirror": "./public/libs/ui-codemirror.js",
    "ui-bootstrap": "./public/libs/ui-bootstrap.js"
  },
  "browserify-shim": "./shim.js",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "scripts": {
    "test": "npm run build && npm run lint && npm run test-unit",
    "start": "bin/strider --cluster=false",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-lib": "npm run build-ts && npm run lint-lib && npm run copy-static-assets",
    "lint-lib": "tsc --noEmit && eslint \"lib/**/*.{js,ts}\" --quiet --fix",
    "watch-lib": "nodemon",
    "build-and-start": "npm run build-ts && npm start",
    "copy-static-assets": "ts-node copy-static-assets.ts",
    "dev": "npm run build-debug && npm run watch-lib",
    "pre": "mkdirp dist/scripts dist/styles",
    "vendor": "bower-installer",
    "styles": "lessc client/styles/strider.less > dist/styles/styles.css",
    "styles-debug": "lessc --source-map client/styles/strider.less dist/styles/styles.css",
    "watch": "npm run pre && watchify -e client/app.js -o dist/scripts/app.js",
    "build": "npm run pre && npm run styles && browserify -d -t [babelify --presets [ es2015 ] --only client] -p [minifyify --output dist/scripts/bundle.map] -e client/app.js -o dist/scripts/app.js",
    "build:ember": "cd client-ember/ && ember build -prod && cd ..",
    "build-debug": "npm run pre && npm run styles-debug && browserify -d -t [babelify --presets [ es2015 ] --only client] -e client/app.js -o dist/scripts/app.js",
    "test-unit": "mocha --recursive test/unit",
    "lint": "eslint *.js client",
    "unitcov": "istanbul cover ./node_modules/mocha/bin/_mocha --recursive test/unit",
    "api-docs": "apidoc -i lib/ -o apidocs/",
    "api-docs:md": "apidoc-markdown -p apidocs -o docs/api.md",
    "docs:build": "docpress b",
    "docs:publish": "git-update-ghpages -b master Strider-CD/strider-cd.github.com _docpress",
    "deploy:docs": "npm run api-docs:md && npm run docs:build && npm run docs:publish",
    "debug": "node --inspect bin/strider --no-cluster",
    "release-build": "npm run build && npm run build:ember && npm run build-lib",
    "prerelease": "npm run release-build",
    "release": "git add dist/ && standard-version -a",
    "postinstall": "npm run build-lib && opencollective postinstall"
  },
  "dependencies": {
    "activedirectory": "^0.7.2",
    "ansi_up": "^4.0.3",
    "async": "^3.1.0",
    "bcryptjs": "^2.1.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.10.0",
    "chalk": "^2.4.2",
    "co-router": "^1.0.3",
    "compression": "^1.2.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.0.0",
    "connect-slashes": "^1.3.1",
    "cookie-parser": "^1.3.3",
    "cors": "^2.5.2",
    "debug": "^4.1.1",
    "errorhandler": "^1.3.0",
    "everypaas": "0.0.8",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "global-tunnel": "^1.2.0",
    "gravatar": "^1.1.0",
    "in-publish": "^2.0.0",
    "lodash": "^4.14.0",
    "md5": "^2.0.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.8.11",
    "morgan": "^1.5.0",
    "morgan-debug": "^2.0.0",
    "node-rsa": "^0.4.0",
    "opencollective": "^1.0.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^1.0.2",
    "pug": "^2.0.0-beta4",
    "rc": "^1.0.3",
    "request": "^2.81.0",
    "semver": "^5.3.0",
    "serve-favicon": "^2.2.0",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.1",
    "sshpk": "^1.8.3",
    "step": "^1.0.0",
    "strider-bitbucket": "^1.2.0",
    "strider-build-badge": "^0",
    "strider-cli": "^2.0.0",
    "strider-custom": "^0.6.0",
    "strider-ecosystem-client": "^1.2.1",
    "strider-email-notifier": "^1.0.0",
    "strider-env": "^0.5.1",
    "strider-extension-loader": "^0.4.5",
    "strider-git": "^1.0.0",
    "strider-github": "^3.0.0",
    "strider-github-status": "^2.0.0",
    "strider-gitlab": "^1.2.3",
    "strider-heroku": "^0.1.1",
    "strider-mailer": "^0.2.0",
    "strider-metadata": "^0.0.3",
    "strider-node": "^1.0.0",
    "strider-python": "^0.2.1",
    "strider-ruby": "^0.0.2",
    "strider-simple-runner": "^1.1.0",
    "strider-slack": "^2.0.0",
    "strider-ssh-deploy": "^1.0.0",
    "strider-webhooks": "^0.1.1",
    "strip-ansi": "^5.2.0",
    "swig": "^0.14.0",
    "thirty-two": "^1.0.1",
    "validator": "^7.0.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.3",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.0",
    "@types/shelljs": "^0.8.6",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "angular": "1.5.7",
    "angular-route": "1.5.7",
    "apidoc": "^0.17.1",
    "apidoc-markdown": "github:zambonilli/node-apidoc-markdown",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "bower-installer": "^1.2.0",
    "browserify": "^16.3.0",
    "browserify-shim": "^3.8.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^5.1.0",
    "docpress": "~0.8.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "expect.js": "^0.3.1",
    "git-update-ghpages": "^1.3.0",
    "httpcheck": "^0.3.0",
    "istanbul": "^0.4.4",
    "jquery": "^3.4.1",
    "less": "^3.9.0",
    "minifyify": "^7.3.3",
    "mkdirp": "^0.5.0",
    "mocha": "^6.0.0",
    "ndb": "^1.1.5",
    "node-mocks-http": "^1.2.4",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.2",
    "standard-version": "^7.1.0",
    "surge": "^0.21.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "watchify": "^3.4.0",
    "wd": "^0.4.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/strider",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  }
}
