    "shelljs": "^0.8.3",
    "sinon": "^7.3.2",
    "standard-version": "^7.1.0",
    "surge": "^0.21.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "wd": "^0.4.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/strider",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  }
}
