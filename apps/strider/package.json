    "@types/mongoose": "^5.7.0",
    "@types/shelljs": "^0.8.6",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "apidoc": "^0.27.1",
    "apidoc-markdown": "^5.2.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "expect.js": "^0.3.1",
